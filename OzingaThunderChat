<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00DU80000088NfZ',
				'ThunderChat',
				'https://ozinga--ozingafull.sandbox.my.site.com/ESWThunderChat1768008021000',
				{
					scrt2URL: 'https://ozinga--ozingafull.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://ozinga--ozingafull.sandbox.my.site.com/ESWThunderChat1768008021000/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>
